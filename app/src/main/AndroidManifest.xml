<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.performance_optimize">

    <application
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HookTest"
        tools:targetApi="31">
        <activity
            android:name=".apksize.ApkSizeOptimizeActivity"
            android:exported="false" />
        <activity
            android:name=".stability.StabilityExampleActivity"
            android:exported="false" />
        <activity
            android:name=".memory.NativeLeakActivity"
            android:exported="false" />
        <activity
            android:name=".memory.JavaLeakActivity"
            android:exported="false" />
        <activity
            android:name=".memory.MemoryExampleActivity"
            android:exported="false" />
        <activity
            android:name="com.example.performance_optimize.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.HookTest.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>